{
  "type": "timeseries",
  "title": true,
  "title_text": "Requests",
  "width": {{width}},
  "height": {{height}},
  "x": {{x}},
  "y": {{y}},
  "tile_def": {
    "viz": "timeseries",
    "requests": [
      {
        "q": "sum:aws.elb.httpcode_target_2xx{name:{{~toLowerCase name~}}}.as_count(), sum:aws.elb.httpcode_target_4xx{name:{{~toLowerCase name~}}}.as_count(), sum:aws.elb.httpcode_target_5xx{name:{{~toLowerCase name~}}}.as_count()",
        "type": "area",
        "style": {
          "palette": "dog_classic",
          "type": "solid",
          "width": "normal"
        },
        "aggregator": "avg",
        "metadata": {
          "sum:aws.elb.httpcode_target_2xx{name:{{~toLowerCase name~}}}.as_count()": {
            "alias": "2xx"
          },
          "sum:aws.elb.httpcode_target_4xx{name:{{~toLowerCase name~}}}.as_count()": {
            "alias": "4xx"
          },
          "sum:aws.elb.httpcode_target_5xx{name:{{~toLowerCase name~}}}.as_count()": {
            "alias": "5xx"
          }
        }
      }
    ],
    "autoscale": true
  },
  "legend": true
}
